#!/bin/sh

DIR=/usr/share/kwtools/functions
BIN_PATH=/usr/bin

for i in kwrecord kwrip kwsnd2sndfm kwvcdburn ; do
	rm -f ${BIN_PATH}/${i} &>/dev/null
done

cd $DIR
for i in kwrecord kwrip kwsnd2sndfm kwvcdburn ; do
	rm -rf ${i}* &>/dev/null
done
rm -rf ${DIR%/*/*}/doc/kwtools-multi &>/dev/null
exit 0
