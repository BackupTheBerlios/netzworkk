#!/bin/sh

DIR=/usr/share/kwtools/functions
SBIN_PATH=/usr/sbin

for i in kwgroup kwgrub kwlogrotate kwwvdialconf \
	kwnetcardconf kwnetconf kwnetcardconf kwparted \
	kwrunlevconf kwssl kwuser kwclock ; do
	rm -f ${SBIN_PATH}/${i} &>/dev/null
done

for i in kwgrub kwnetcardconf kwlogrotate kwrunlevconf kwssl kwwvdialconf ; do
	rm -rf ${DIR}/${i}* &>/dev/null
done
exit 0

