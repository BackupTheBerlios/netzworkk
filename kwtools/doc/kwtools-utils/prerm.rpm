#!/bin/sh

DIR=/usr/share/kwtools/functions
BIN_PATH=/usr/bin

for i in kwbackup kwgpgcrypt kwmuttprintcf kwpwsafe kwvmstat ; do
	rm -f ${BIN_PATH}/${i} &>/dev/null
done

cd $DIR
for i in kwbackup kwgpgcrypt kwmuttprintcf kwpwsafe kwvmstat ; do
	rm -rf ${i}* &>/dev/null
done
rm -rf ${DIR%/*/*}/doc/kwtools-utils &>/dev/null
exit 0

